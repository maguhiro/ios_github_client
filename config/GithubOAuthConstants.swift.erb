import Foundation

struct GithubOAuthConstants {
  private init() {}

  static let clientID: String = "<%= github["client_id"] %>"
  static let clientSecret: String = "<%= github["client_secret"] %>"
  static let callbackURL: URL = URL(string: "<%= github["callback_url"] %>")!
  static let scope: String = "<%= github["scope"] %>"
  static let state: String = "<%= github["state"] %>"
}
